{
  "$schema": "https://json-schema.org/draft-04/schema",
  "Logging": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Serilog.AspNetCore.RequestLoggingMiddleware": "Information"
      }
    },
    "Using": [
      "Serilog.Sinks.Console",
      "Extense.Logging.Enrichers.DistributedTracing"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "WithDistributedTracing"
    ]
  }
}
